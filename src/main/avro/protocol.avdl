@namespace("com.github.dfauth.socketio")
protocol Example {

  record Envelope{
    string id;
    map<string> metadata;
    string schemaRegistryTopic;
    bytes payload;
  }

  record TestEvent {
    string name;
    int ackId;
    string payload;
  }

  record AnotherTestEvent {
    string name;
    int ackId;
    int payload;
    timestamp_ms ts;
  }

}
